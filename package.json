{
  "name": "equinusocio-blog",
  "version": "2.0.0",
  "description": "A static blog based on Contentful and Nuxt",
  "author": "Mattia Astorino",
  "private": true,
  "keywords": [
    "nuxt",
    "vue",
    "babel",
    "postcss",
    "contentful",
    "stylelint"
  ],
  "scripts": {
