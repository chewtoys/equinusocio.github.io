    "@nuxtjs/google-analytics": "2.2.0",
    "axios": "0.18.0",
    "babel-eslint": "^10.0.1",
    "body-scroll-lock": "2.6.1",
    "contentful": "^7.6.0",
    "cssnano": "^4.1.10",
    "cssnano-preset-advanced": "4.0.7",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.1.2",
