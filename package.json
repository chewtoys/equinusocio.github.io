{
  "name": "contentful-blog-in-5-min",
  "version": "1.1.1",
  "description": "A static blog based on Contentful",
  "author": "stefan judis <stefanjudis@gmail.com>",
  "private": true,
  "dependencies": {
    "contentful": "7.0.5",
    "contentful-management": "5.4.0",
    "nuxt": "2.4.3",
    "prismjs": "1.15.0",
    "remove": "0.1.5",
    "stylelint-config-standard": "^18.2.0",
    "vue-markdown": "^2.1.3"
  },
  "scripts": {
    "dev": "nuxt",
    "build": "BASE_URL=https://equinsuocha.io nuxt build",
    "start": "nuxt start",
    "generate": "BASE_URL=https://equinsuocha.io nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "deploy": "./node_modules/.bin/push-dir --dir=dist --branch=master --cleanup",
    "release": "npm run build && npm run generate && npm run deploy"
  },
  "devDependencies": {
    "@mapbox/stylelint-processor-arbitrary-tags": "0.2.0",
    "@native-elements/ne-button": "0.4.0",
    "@nuxtjs/google-analytics": "2.0.2",
    "@nuxtjs/pwa": "3.0.0-beta.14",
    "babel-eslint": "10.0.1",
    "babel-runtime": "6.26.0",
    "body-scroll-lock": "2.6.1",
    "contentful-import": "7.4.3",
    "cssnano": "4.1.8",
    "cssnano-preset-advanced": "4.0.6",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "5.11.1",
    "eslint-config-prettier": "3.3.0",
    "eslint-config-standard": "12.0.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-html": "5.0.0",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-standard": "4.0.0",
    "eslint-plugin-vue": "5.0.0",
    "gh-pages": "2.0.1",
    "listr": "0.14.3",
    "modern-normalize": "0.5.0",
    "node-fetch": "2.3.0",
    "postcss-easy-import": "3.0.0",
    "postcss-html": "0.34.0",
    "postcss-mixins": "6.2.1",
    "postcss-preset-env": "6.5.0",
    "postcss-reporter": "6.0.0",
    "postcss-responsive-type": "1.0.0",
    "postcss-syntax": "0.34.0",
    "prettier": "1.15.3",
    "push-dir": "0.4.1",
    "rimraf": "^2.6.1",
    "stylelint": "9.9.0",
    "stylelint-webpack-plugin": "0.10.5",
    "tar": "^4.0.1",
    "vue-lazyload": "1.2.6",
    "vue-media": "1.1.1",
    "vue-scroll-reveal": "1.0.11"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 2 Edge version"
  ]
}
