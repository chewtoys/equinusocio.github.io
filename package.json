{
  "name": "contentful-blog-in-5-min",
  "version": "1.1.1",
  "description": "A static blog based on Contentful",
  "author": "stefan judis <stefanjudis@gmail.com>",
  "private": true,
  "dependencies": {
    "contentful": "^4.3.0",
    "contentful-management": "^3.4.0",
    "nuxt": "^1.0.0-rc4",
    "prismjs": "1.15.0",
    "remove": "0.1.5",
    "stylelint-config-standard": "^18.2.0",
    "vue-markdown": "^2.1.3"
  },
  "scripts": {
    "dev": "nuxt",
    "build": "BASE_URL=https//equinsuocha.io nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "deploy": "nuxt generate && now dist",
    "import-data": "node ./bin/download-content-model.js && contentful-import --content-file ./data/blog/contentful-export.json"
  },
  "devDependencies": {
    "@mapbox/stylelint-processor-arbitrary-tags": "0.2.0",
    "babel-eslint": "^7.1.1",
    "contentful-import": "7.4.0",
    "cssnano": "4.1.0",
    "cssnano-preset-advanced": "4.0.0",
    "cz-conventional-changelog": "^2.0.0",
    "element-ui": "2.4.7",
    "eslint": "^3.15.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^2.0.0",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-standard": "^2.0.1",
    "gh-pages": "2.0.0",
    "listr": "^0.12.0",
    "node-fetch": "^1.7.3",
    "now": "^5.3.0",
    "postcss-easy-import": "3.0.0",
    "postcss-mixins": "6.2.0",
    "postcss-normalize": "7.0.1",
    "postcss-preset-env": "5.3.0",
    "postcss-reporter": "6.0.0",
    "rimraf": "^2.6.1",
    "stylelint": "9.5.0",
    "tar": "^4.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
