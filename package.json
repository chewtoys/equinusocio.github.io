{
  "name": "equinsuocha.io",
  "scripts": {
    "start": "./node_modules/.bin/hexo clean && ./node_modules/.bin/hexo server --config config-dev.yml",
    "build": "./node_modules/.bin/hexo clean && ./node_modules/.bin/hexo generate --config config-dev.yml,config-prod.yml",
    "deploy": "./node_modules/.bin/hexo deploy --config config-dev.yml,config-prod.yml",
    "release": "npm run build && npm run deploy"
  },
  "version": "0.0.1",
  "private": true,
  "hexo": {
    "version": "3.7.0"
  },
  "dependencies": {
    "hexo": "^3.7.1",
    "hexo-all-minifier": "^0.5.2",
    "hexo-autoprefixer": "^1.0.0",
    "hexo-deployer-git": "git+ssh://git@github.com/hexojs/hexo-deployer-git.git",
    "hexo-featured-image": "^0.2.1",
    "hexo-generator-archive": "^0.1.4",
    "hexo-generator-category": "^0.1.3",
    "hexo-generator-index": "^0.2.0",
    "hexo-generator-tag": "^0.2.0",
    "hexo-renderer-ejs": "^0.3.0",
    "hexo-renderer-marked": "^0.3.0",
    "hexo-renderer-stylus": "^0.3.1",
    "hexo-server": "^0.2.0"
  }
}