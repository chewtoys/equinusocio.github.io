  <script>
    document.addEventListener("DOMContentLoaded",function(){var a=[].slice.call(document.querySelectorAll("img.Lazy"));if("IntersectionObserver"in window){let b=new IntersectionObserver(function(c){c.forEach(function(e){if(e.isIntersecting){let f=e.target;f.src=f.dataset.src,f.srcset=f.dataset.srcset,f.classList.remove("Lazy"),b.unobserve(f)}})});a.forEach(function(c){b.observe(c)})}else{let b=!1;const c=function(){!1==b&&(b=!0,setTimeout(function(){a.forEach(function(d){d.getBoundingClientRect().top<=window.innerHeight&&0<=d.getBoundingClientRect().bottom&&"none"!==getComputedStyle(d).display&&(d.src=d.dataset.src,d.srcset=d.dataset.srcset,d.classList.remove("Lazy"),a=a.filter(function(e){return e!==d}),0===a.length&&(document.removeEventListener("scroll",c),window.removeEventListener("resize",c),window.removeEventListener("orientationchange",c)))}),b=!1},200))};document.addEventListener("scroll",c),window.addEventListener("resize",c),window.addEventListener("orientationchange",c)}});
